{
    "name": "abikit",
    "version": "0.2.2",
    "description": "Multi-language SDK generator for smart contracts with unified SDK classes (TypeScript/Python)",
    "main": "dist/index.js",
    "bin": {
        "abikit": "dist/cli.js",
        "contract-sdk-gen": "dist/cli.js"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "clean": "rm -rf dist",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write 'src/**/*.ts'"
    },
    "keywords": [
        "ethereum",
        "smart-contracts",
        "sdk",
        "code-generation",
        "abi",
        "viem",
        "web3.py",
        "typescript",
        "python"
    ],
    "author": "ahloop",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ahloop/abikit.git",
        "directory": "cli"
    },
    "homepage": "https://abikit.ahloop.com",
    "bugs": {
        "url": "https://github.com/ahloop/abikit/issues"
    },
    "dependencies": {
        "commander": "^11.1.0",
        "ejs": "^3.1.9",
        "ajv": "^8.12.0",
        "js-yaml": "^4.1.0",
        "chalk": "^4.1.2",
        "chokidar": "^3.5.3",
        "ethers": "^5.7.2"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/js-yaml": "^4.0.9",
        "@types/ejs": "^3.1.5",
        "@types/jest": "^29.5.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.0.0",
        "jest": "^29.5.0",
        "ts-jest": "^29.1.0",
        "typescript": "^5.0.0",
        "prettier": "^3.0.0"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}